{
    "name": "xml-comparer-tool",
    "version": "1.0.0",
    "description": "Este proyecto es una aplicación web que permite comparar estructuras XML de manera visual. Utiliza varias tecnologías como HTML, CSS, JavaScript, Bootstrap, CodeMirror y jsdiff para proporcionar una interfaz amigable y funcional.",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "install-java": "apt-get update && apt-get install -y openjdk-11-jre",
        "start:parcel": "parcel build public/index.html --dist-dir dist --no-cache",
        "start:server": "node server.js",
        "start": "concurrently \"npm run dev && npm run start:server\"",
        "build": "npx webpack --mode production --config webpack.config.js",
        "dev": "npx webpack --mode development --config webpack.config.dev.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JavierFernandezPalma/XMLComparerTool.git"
    },
    "dependencies": {
        "buffer": "^6.0.3",
        "cors": "^2.8.5",
        "diff": "^5.2.0",
        "express": "^4.19.2",
        "libxmljs2": "^0.35.0",
        "multer": "^1.4.4",
        "node-fetch": "^3.3.2",
        "node-forge": "^1.3.1",
        "shepherd.js": "^13.0.3",
        "stream-browserify": "^3.0.0",
        "timers-browserify": "^2.0.12",
        "tv4": "^1.3.0",
        "xml2js": "^0.6.2",
        "xmldom": "^0.6.0",
        "xmllint": "^0.1.1",
        "xsd-schema-validator": "^0.9.0"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/plugin-transform-runtime": "^7.25.4",
        "@babel/preset-env": "^7.25.4",
        "babel-loader": "^9.2.1",
        "concurrently": "^8.2.2",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.6.0",
        "mini-css-extract-plugin": "^2.9.1",
        "terser-webpack-plugin": "^5.3.10",
        "url-loader": "^4.1.1",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4"
    },
    "keywords": [],
